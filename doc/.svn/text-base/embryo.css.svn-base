h1
{
 text-align: center;
 color: #333333
}
h2
{
 text-align: left;
 color: #333333
}
h3
{
 text-align: left;
 color: #333333
}
a:link
{
 text-decoration: none;
 color: #444444;
 font-weight: bold;
}
a:visited
{
 text-decoration: none;
 color: #666666;
 font-weight: bold;
}
a:hover
{
 text-decoration: none;
 color: #000000;
 font-weight: bold;
}
a.nav:link
{
 text-decoration: none;
 color: #444444;
 font-weight: normal;
}
a.nav:visited
{
 text-decoration: none;
 color: #666666;
 font-weight: normal;
}
a.nav:hover
{
 text-decoration: none;
 color: #000000;
 font-weight: normal;
}
a.qindex:link
{
 text-decoration: none;
 color: #444444;
 font-weight: normal;
}
a.qindex:visited
{
 text-decoration: none;
